<%= form_for(@article) do |f| %>
	
	<div class="field">
		<%= f.label :title %><br />
		<%= f.text_field :title %>
		</div>
		<div class="field">
		<%= f.label :content %><br />
			<%= f.text_area :content %>
		</div>
		<%= f.file_field :articlepic%>
		<div class="actions">
		<%= f.submit "Create" %>
	</div>
<% end %>